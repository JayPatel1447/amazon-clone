oute>\n       \n      </Switch>\n    </div>\n    </Router>\n  );\n}\n\nexport default App;\n","C:\\Users\\jaypa\\OneDrive\\Desktop\\React Project\\amazon-clone\\src\\reportWebVitals.js",[],"C:\\Users\\jaypa\\OneDrive\\Desktop\\React Project\\amazon-clone\\src\\Components\\Header\\Header.js",["66","67"],"import React from 'react'\r\nimport './Header.css'\r\n\r\nimport SearchIcon from '@material-ui/icons/Search';\r\nimport ShoppingBasketIcon from '@material-ui/icons/ShoppingBasket'\r\nimport {Link} from 'react-router-dom'\r\nimport { useStateValue } from '../StateProvider/StateProvider';\r\nimport {auth} from \"../../firebase\"\r\n\r\nfunction Header() {\r\n\r\n    const [{basket, user}, dispatch]  = useStateValue();\r\n\r\n    const handleAuthentication = () => {\r\n            if (user){\r\n                auth.signOut();\r\n            }\r\n    }\r\n\r\n    return (\r\n        <div className=\"header\">\r\n            <Link to=\"/\">\r\n            <img className=\"header__logo\"  src=\"https://pngimg.com/uploads/amazon/amazon_PNG11.png\"/>\r\n            </Link>\r\n            <div className=\"header__search\">\r\n                <input className=\"header__searchInput\" type=\"text\"/>\r\n                <SearchIcon className=\"header__searchIcon\" type=\"text\"/>      \r\n             </div>\r\n\r\n            <div className=\"header__nav\">\r\n            <Link to={!user && '/login'} >\r\n                <div onClick={handleAuthentication} className=\"header__option\">\r\n                    <span className=\"header__optionLineOne\">\r\n                        {user ? user.email : 'Hello Guest'}\r\n                    </span>\r\n                    \r\n                    <span style={{color: 'white'}} className=\"header__optionLineTwo\">\r\n                        {user ? 'Sign Out' : 'Sign In'}\r\n                    </span>\r\n                   \r\n                </div>\r\n                </Link>\r\n                \r\n                <div className=\"header__option\">\r\n                <span className=\"header__optionLineOne\">\r\n                        Returns\r\n                    </span>\r\n                    <span className=\"header__optionLineTwo\">\r\n                        & Orders\r\n                    </span>\r\n                </div>\r\n                <div className=\"header__option\">\r\n                <span className=\"header__optionLineOne\">\r\n                        Your \r\n                    </span>\r\n                    <span className=\"header__optionLineTwo\">\r\n                        Prime\r\n                    </span>\r\n                </div>\r\n                <Link to=\"/checkout\">\r\n                <div className=\"header__optionBasket\">\r\n                    <ShoppingBasketIcon />\r\n                    <span className=\"header__optionLineTwo header_basketCount\">{basket?.length}</span>\r\n                </div>\r\n                </Link>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Header\r\n","C:\\Users\\jaypa\\OneDrive\\Desktop\\React Project\\amazon-clone\\src\\Components\\Home\\Home.js",[],"C:\\Users\\jaypa\\OneDrive\\Desktop\\React Project\\amazon-clone\\src\\Components\\Product\\Product.js",[],["68","69"],"C:\\Users\\jaypa\\OneDrive\\Desktop\\React Project\\amazon-clone\\src\\Components\\Checkout\\Checkout.js",["70","71"],"import React from 'react'\r\nimport { useStateValue } from '../StateProvider/StateProvider'\r\nimport \"./Checkout.css\"\r\nimport CheckoutProduct from './CheckoutProduct';\r\n\r\nimport Subtotal from \"./Subtotal/Subtotal\"\r\nfunction Checkout() {\r\n\r\n    const [{basket}, dispatch] = useStateValue();\r\n\r\n\r\n    return (\r\n        <div className=\"checkout\">\r\n            <div className=\"checkout__left\">\r\n                <img src=\"https://images-na.ssl-images-amazon.com/images/G/02/UK_CCMP/TM/OCC_Amazon1._CB423492668_.jpg\" className=\"checkout__add\"/>\r\n        \r\n            <div>\r\n            